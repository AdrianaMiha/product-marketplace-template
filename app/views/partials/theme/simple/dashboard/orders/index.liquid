<h5><a href="/dashboard">{% echo 'app.dashboard' | t %}</a> / {% echo 'app.orders' | t %}</h5>

<table>
  <thead>
    <tr>
      <th>{{ "app.item_id" | t }}</th>
      <th>{{ "app.created_at" | t }}</th>
      <th>{{ "app.buyer" | t }}</th>
      <th>{{ "app.status" | t }}</th>
      <th>{{ "app.paid_at" | t }}</th>
    </tr>
  </thead>
  {% for order in orders.results %}
    <tr>
      <td>{% echo order.created_at | l %}</td>
      <td>{% print order.properties.item_id %}</td>
      <td>{% print order.properties.buyer %}</td>
      <td>{% print order.properties.status %}</td>
      <td>{% echo order.properties.paid_at %}</td>
    </tr>
  {% endfor %}
</table>
