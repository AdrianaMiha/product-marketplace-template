{% liquid
  graphql r = 'item_statuses/create', args: object

  if r.errors
    log object, type: 'errors.graphql.invalid item-statuses-create-execute'
    log r.errors, type: 'errors.graphql.invalid item-statuses-create-execute'
    assign object = object | add_hash_key: 'errors', r.errors
    assign object = object | add_hash_key: 'valid', false
  else
    assign object = r.model_create
    assign object = object | add_hash_key: 'valid', true
  endif

  export object
%}
