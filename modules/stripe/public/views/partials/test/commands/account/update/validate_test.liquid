{% parse_json data %}
  {
    "payouts_enabled": false,
    "charges_enabled": false
  }
{% endparse_json %}
{% parse_json object %}
  {
    "id": "123",
    "raw_data": {{ data }}
  }
{% endparse_json %}

{% liquid
  include 'modules/stripe/lib/account/update/validate', object: object

  if context.exports.object.valid != true
    include 'test/register_error', contract: contract, field_name: 'base', key: 'test.should.be_valid'
  endif

  if context.exports.object.account_state != 'pending'
    include 'test/register_error', contract: contract, field_name: 'account_state', message: 'wrong state'
  endif
%}
