{% liquid
  function_rc response = 'modules/stripe/lib/stripe_webhook/create/call', event: event, path: path
  parse_json webhook
    include 'modules/stripe/lib/webhook_endpoints/create/build', response: response, event: event
  endparse_json
  if webhook.valid
    graphql result = 'modules/stripe/webhook_endpoint_create', args: webhook
    log result, type: 'webhook_endpoints/create model_create'
  else
    log webhook, type: 'webhook_endpoints/create invalid'
  endif
%}
